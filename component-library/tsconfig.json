{
  "compilerOptions": {
    "incremental": true,
    "module": "ESNext",
    "lib": ["ES2018", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "alwaysStrict": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "types": [],
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}
